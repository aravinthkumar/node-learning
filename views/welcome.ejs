<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/user.css">
    </head>

    <body>
        <%- include('includes/navigation.ejs') %>
            <main>
                <% if (user.length> 0) { %>
                    <div class="grid">
                        <% for (let us of user) { %>
                            <article class="card user-item">
                                <header class="card__header">
                                    <h1 class="user__title">
                                        <%= us.title %>
                                    </h1>
                                </header>
                            </article>
                            <% } %>
                    </div>
                    <% } else { %>
                        <h1>No User Found!</h1>
                        <% } %>
            </main>
            <%- include('includes/end.ejs') %>